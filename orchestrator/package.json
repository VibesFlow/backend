  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.32.0",
    "alith": "^0.12.3",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "cors": "^2.8.5",
    "ws": "^8.18.3",
    "uuid": "^13.0.0"
  },
  "optionalDependencies": {
    "@lazai-labs/alith-linux-x64-gnu": "latest"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.15",
    "@types/node": "^24.10.2",
    "@types/ws": "^8.18.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
