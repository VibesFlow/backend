{
  "name": "alith-orchestrator",
  "version": "1.0.0",
  "description": "Alith AI orchestrator for real-time DJ Set generation with robust buffering and no latency",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "test": "node test.js",
